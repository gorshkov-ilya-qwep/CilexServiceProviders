{
    "name": "kamilsk/cilex-service-providers",
    "type": "library",
    "description": "The revised set of service providers for Cilex.",
    "homepage": "http://kamilsk.github.io/CilexServiceProviders/",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Samigullin",
            "email": "kamil@samigullin.info"
        }
    ],
    "require": {
        "php": ">=5.5",
        "cilex/cilex": "~1.0",
        "symfony/config": "~2.5",
        "symfony/yaml": "~2.5"
    },
    "require-dev": {
        "doctrine/dbal": "~2.0",
        "monolog/monolog": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "doctrine/dbal": "for DoctrineServiceProvider (~2.0 is recommended)",
        "monolog/monolog": "for MonologServiceProvider (~1.0 is recommended)"
    },
    "autoload": {
        "psr-4": {
            "OctoLab\\Cilex\\": "src/"
        }
    }
}
