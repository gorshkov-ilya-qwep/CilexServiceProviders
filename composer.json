{
  "name": "kamilsk/cilex-service-providers",
  "type": "library",
  "description": "Revised set of service providers for Cilex.",
  "keywords": ["php cli", "cli", "cilex"],
  "homepage": "https://github.com/kamilsk/CilexServiceProviders",
  "license": "MIT",
  "authors": [
    { "name": "Kamil Samigullin", "email": "kamil@samigullin.info", "role": "lead" }
  ],
  "require": {
    "php": ">=5.5",
    "cilex/cilex": "~1.0",
    "kamilsk/common": "^2.1.4",
    "kamilsk/kilex": "~1.0"
  },
  "require-dev": {
    "doctrine/migrations": "~1.0",
    "jolicode/jolinotif": "~1.0",
    "league/climate": "~3.0",
    "php-school/cli-menu": "~1.0",
    "symfony/config": "~2.0|~3.0",
    "symfony/event-dispatcher": "~2.0|~3.0",
    "symfony/monolog-bridge": "~2.0|~3.0",
    "symfony/yaml": "~2.0|~3.0"
  },
  "suggest": {
    "doctrine/dbal": "for DoctrineServiceProvider (~2.0 is recommended)",
    "doctrine/migrations": "optional for use OctoLab\\**\\Doctrine\\*Migration* (~1.0 is recommended)",
    "jolicode/jolinotif": "optional for show desktop notification (~1.0 is recommended)",
    "league/climate": "optional for colorize output (~3.0 is recommended)",
    "monolog/monolog": "for use MonologServiceProvider (~1.0 is recommended)",
    "php-school/cli-menu": "for PresentCommand (~1.0 is recommended, PHP 5.6+ is required)",
    "symfony/config": "for use ConfigServiceProvider (~3.0 is recommended)",
    "symfony/event-dispatcher": "needed when using log messages in console commands (~3.0 is recommended)",
    "symfony/monolog-bridge": "optional for MonologServiceProvider (~3.0 is recommended)",
    "symfony/yaml": "for use ConfigServiceProvider (~3.0 is recommended)"
  },
  "autoload": {
    "psr-4": { "OctoLab\\Cilex\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "OctoLab\\Cilex\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  },
  "support": {
    "irc": "irc://irc.gitter.im/kamilsk/small-tools",
    "issues": "https://github.com/kamilsk/CilexServiceProviders/issues"
  }
}
